//- header.pug
header.navbar.navbar-dark.bg-dark
    .container
        a.navbar-brand(href="#") F8 education
        nav
            ul.nav
                li.nav-item
                    a.nav-link(href=`${prefixAdmin}/dashboard`) Home
                li.nav-item
                    a.nav-link(href="/Đăng Khóa Học") Link
                li.nav-item.dropdown
                    a.nav-link.dropdown-toggle(
                        href="#", 
                        role="button", 
                        data-bs-toggle="dropdown", 
                        aria-expanded="false"
                    )
                        img(
                            src="data:image/jpeg;base64,", 
                            alt="User Avatar", 
                            class="rounded-circle me-2", 
                            style="width: 30px; height: 30px; object-fit: cover;"
                        ) 
                        | 
                    ul.dropdown-menu
                        li
                            a.dropdown-item(href="#") Action
                        
                        if(role.permissions.includes("product-category_view"))
                            li
                                a.dropdown-item(href=`${prefixAdmin}/product-category/`) Quản lý danh mục 
                        li
                            a.dropdown-item(href=`${prefixAdmin}/roles`) Danh Sách Nhóm Quyền 

                            a.dropdown-item(href=`${prefixAdmin}/roles/permissions`) Phân Quyền 
                        li
                            hr.dropdown-divider
                        li
                            a.dropdown-item(href=`${prefixAdmin}/account`) Quản lý tài khoản
                        li
                            a.dropdown-item(href=`${prefixAdmin}/auth/logout`) Đăng xuất  
